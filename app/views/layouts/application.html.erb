<!DOCTYPE html>
<html>
<head>
  <title>Cloudchart</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= raw "<div class='alert alert-info'><a class='close' data-dismiss='alert' href='#'>×</a>#{notice}</div>" if notice.present? %>
<%= raw "<div class='alert alert-error'><a class='close' data-dismiss='alert' href='#'>×</a>#{alert}</div>" if alert.present? %>

<%= yield %>

</body>
</html>
